u4e8e\u4e3b\u7ba1\u610f\u89c1\u3002 6.\u4e8e\u4e3b\u7ba1\u5ba1\u6279\u6210\u529f 4.\u6240\u9700\u6750\u6599","title":"\u573a\u666f5"},{"location":"scene5/#5","text":"1.\u89d2\u8272 1.\u62a5\u4fee\u4eba\uff1a\u5c0f\u767d 2.\u884c\u653f\u90e8\uff1a\u8521\u90e8\u957f 3.\u6280\u672f\u90e8\uff1a\u4fce\u90e8\u957f 4.\u6280\u672f\u90e8\u7ef4\u62a4\u4eba\u5458\uff1a\u5c0f\u5218 5.\u4e3b\u7ba1\u9886\u5bfc\uff1a\u4e8e\u4e3b\u7ba1 2.\u6d41\u7a0b 1.\u62a5\u4fee\u4eba\u9489\u9489\u586b\u5199\u7533\u8bf7 2.\u884c\u653f\u90e8\u5ba1\u6838\uff0c\u901a\u77e5\u6280\u672f\u90e8 3.\u6280\u672f\u90e8\u6d3e\u51fa\u7ef4\u62a4\u4eba\u5458\u5e76\u586b\u5199\"\u8bbe\u5907\u7ef4\u62a4\u8868\" 4.\u8bbe\u5907\u65e0\u6cd5\u4fee\u590d\u65f6\uff0c\u62a5\u4fee\u4eba\u586b\u5199\"\u8bbe\u5907\u62a5\u5e9f\u7533\u8bf7\u8868\"\uff0c\u6280\u672f\u90e8\u95e8\u9a8c\u8bc1\u540e\u7531\u4e3b\u7ba1\u9886\u5bfc\u5ba1\u6279\u3002 3.\u8349\u7a3f 1.\u5c0f\u767d\u5728\u5f00\u53d1\u65f6\u7535\u8111\u53d1\u751f\u9ed1\u5c4f\uff0c\u81ea\u5df1\u591a\u6b21\u91cd\u542f\u540e\u65e0\u679c\uff0c\u5728\u65c1\u4eba\u529d\u8bf4\u4e0b\uff0c\u6253\u7b97\u7533\u8bf7\u7ef4\u4fee\uff0c\u4e8e\u662f\u5728\u9489\u9489\u4e0a\u5148\u8fdb\u884c\u4e86\u7533\u8bf7\u4fdd\u4fee\u7684\u586b\u5199\u3002 2.\u8521\u90e8\u957f\u5728\u9489\u9489\u4e0a\u8fdb\u884c\u4e86\u5ba1\u6838\u7136\u540e\u901a\u77e5\u4e86\u6280\u672f\u90e8\u95e8\u3002 3.\u4fce\u7ec4\u957f\u6536\u5230\u4e86\u884c\u653f\u90e8\u7684\u901a\u77e5\u540e\u6d3e\u51fa\u4e86\u7ef4\u62a4\u4eba\u5458\u5c0f\u5218\u53bb\u7ef4\u4fee\uff0c\u5c0f\u5218\u5e76\u4e14\u586b\u5199\u4e86\u8bbe\u5907\u7ef4\u62a4\u8868\u3002\u5728\u7ef4\u4fee\u4e2d\u53d1\u73b0\u8bbe\u5907\u5df2\u7ecf\u65e0\u6cd5\u4fee\u590d\u540e\u3002\u8ba9\u5c0f\u767d\u586b\u5199\u8bbe\u5907\u62a5\u5e9f\u7533\u8bf7\u8868\u3002 4.\u5c0f\u767d\u586b\u5199\u4e86\u8bbe\u5907\u62a5\u5e9f\u7533\u8bf7\u8868\uff0c\u5e76\u4ea4\u7ed9\u4e86\u6280\u672f\u90e8\u95e8\u4fce\u7ec4\u957f\u3002 5.\u4fce\u7ec4\u957f\u8fdb\u884c\u4e86\u9a8c\u8bc1\uff0c\u786e\u5b9e\u9700\u8981\u62a5\u5e9f\u540e\u7136\u540e\u53bb\u7533\u8bf7\u4e8e\u4e3b\u7ba1\u610f\u89c1\u3002 6.\u4e8e\u4e3b\u7ba1\u5ba1\u6279\u6210\u529f 4.\u6240\u9700\u6750\u6599","title":"\u573a\u666f5\uff1a\u8bbe\u5907\u7ef4\u4fee"},{"location":"scene8/","text":"\u573a\u666f8\uff1a\u5de5\u4f5c\u8fdb\u5ea6\u6c47\u62a5 \u90ae\u4ef6\u5185\u5bb9\uff1a \u6536\u4ef6\u4eba\uff1a\u5f00\u53d1\u90e8\u90e8\u957f \u4e3b\u9898\uff1a\u5c0f\u767d-\u5de5\u4f5c\u5468\u62a52020\u5e747\u67086\u65e5\u81f32020\u5e747\u670810\u65e5 \u6b63\u6587\uff1a \u5c0a\u656c\u7684\u5f00\u53d1\u90e8\u90e8\u957f\uff1a \u60a8\u597d\uff01 \u5f15\u8a00\uff1a\u9879\u76ee\u540d\u79f0\u4e3ax\u5e02\u533b\u4fdd\u4e00\u4f53\u5316\u53d7\u7406\u5e73\u53f0\u5f00\u53d1\uff0c\u56e2\u961f\u6210\u5458\u5305\u62ec\u7ec4\u957f\u8001\u5f20\uff0c\u7ec4\u5458\u5c0f\u674e\u3001\u5c0f\u90ed\u3001\u5c0f\u5f20\u3001\u5c0f\u5b54\u3001\u5c0f\u8bb8\u3001\u5c0f\u6731\u3002 \u5728\u8fc7\u53bb\u4e00\u5468\u7684\u5de5\u4f5c\u4e2d\uff0c\u6211\u5bf9\u9879\u76ee\u8fdb\u884c\u4e86\u8be6\u7ec6\u7684\u5f00\u53d1\u5de5\u4f5c\u3002\u5177\u4f53\u60c5\u51b5\u5982\u4e0b: 1.\u517b\u8001\u7f34\u8d39\u51ed\u8bc1\u6253\u5370\u529f\u80fd:\u5df2\u5b8c\u6210\uff0c\u65e0\u5ef6\u8fdf\u3002 2.\u5931\u4e1a\u91d1\u7ebf\u4e0a\u7533\u9886\u529f\u80fd: \u5df2\u5b8c\u6210\uff0c\u65e0\u5ef6\u8fdf\u3002 3.\u518d\u5c31\u4e1a\u4e0d\u6ee1\u4e00\u5e74\u5f85\u9047\u6838\u5b9a\u529f\u80fd:\u5df2\u5b8c\u6210\uff0c\u65e0\u5ef6\u8fdf\u3002 4.\u6280\u80fd\u63d0\u5347\u8865\u8d34\u7533\u9886\u529f\u80fd:\u5df2\u5b8c\u6210 90%\u7684\u5f00\u53d1\u5de5\u4f5c\uff0c\u5269\u4f59 10%\u9700\u8981\u89e3\u51b3\u8bc1\u4e66\u4fe1\u606f\u83b7\u53d6\u63a5\u53e3\u7684\u95ee\u9898\u3002 \u672c\u5468\u9047\u5230\u7684\u5173\u952e\u95ee\u9898\uff1a\u5931\u4e1a\u4fdd\u9669\u670d\u52a1-\u6280\u80fd\u63d0\u5347\u8865\u8d34\u7533\u9886\u6a21\u5757\u4e2d\u8bc1\u4e66\u4fe1\u606f\u83b7\u53d6\u63a5\u53e3\u672a\u77e5\u3002\u4e3a\u4e86\u89e3\u51b3\u8fd9\u4e2a\u95ee\u9898\uff0c\u6211\u8ba1\u5212\u5728\u4e0b\u5468\u7740\u624b\u7814\u7a76\u5e76\u5bfb\u627e\u89e3\u51b3\u65b9\u6848\u3002 \u6211\u7684\u4e0b\u5468\u8ba1\u5212\uff1a\u5f00\u53d1\u517b\u8001\u4fdd\u9669\u670d\u52a1\u8f6c\u5165\u7533\u8bf7\u529f\u80fd\u5e76\u5bf9\u672c\u5468\u5df2\u5f00\u53d1\u5b8c\u6210\u529f\u80fd\u8fdb\u884c\u5355\u5143\u6d4b\u8bd5\u53ca bug \u4fee\u6539\u3002 \u9664\u4e86\u4ee5\u4e0a\u7684\u5de5\u4f5c\u9879\u76ee,\u6211\u8fd8\u53c2\u4e0e\u4e86\u56e2\u961f\u7684\u8ba8\u8bba\u4f1a\u8bae,\u5e76\u8fdb\u884c\u4e86\u5173\u4e8e\u65b0\u529f\u80fd\u7684\u57f9\u8bad\u3002\u9644\u4ef6\u63d0\u4f9b\u4e86\u672c\u5468\u5de5\u4f5c\u7684\u8be6\u7ec6\u62a5\u544a\uff0c\u8bf7\u60a8\u548c\u56e2\u961f\u6210\u5458\u53c2\u8003\u3002 \u795d\u60a8\u5de5\u4f5c\u6109\u5feb! 2020\u5e747\u670810\u65e5 \u5f00\u53d1\u4eba\u5458\u5c0f\u767d \u5f00\u53d1\u90e8\u533b\u4fdd\u7ec4 \u6cb3\u5317\u4e1c\u8f6f\u8f6f\u4ef6\u6709\u9650\u516c\u53f8 \u5355\u4f4d\u5730\u5740\uff1a\u79e6\u7687\u5c9b\u5e02\u7ecf\u6d4e\u6280\u672f\u5f00\u53d1\u533a\u6cf0\u5c71\u8def143\u53f7\u7efc\u5408\u5b9e\u9a8c\u697c15\u697c \u90ae\u7f16\uff1a066004 \u8054\u7cfb\u7535\u8bdd\uff1a0335-8529605 \u79fb\u52a8\u7535\u8bdd\uff1a15865478965 \u7535\u5b50\u90ae\u4ef6\uff1axiaobai@neuqsoft.com","title":"\u573a\u666f8"},{"location":"scene8/#8","text":"\u90ae\u4ef6\u5185\u5bb9\uff1a \u6536\u4ef6\u4eba\uff1a\u5f00\u53d1\u90e8\u90e8\u957f \u4e3b\u9898\uff1a\u5c0f\u767d-\u5de5\u4f5c\u5468\u62a52020\u5e747\u67086\u65e5\u81f32020\u5e747\u670810\u65e5 \u6b63\u6587\uff1a \u5c0a\u656c\u7684\u5f00\u53d1\u90e8\u90e8\u957f\uff1a \u60a8\u597d\uff01 \u5f15\u8a00\uff1a\u9879\u76ee\u540d\u79f0\u4e3ax\u5e02\u533b\u4fdd\u4e00\u4f53\u5316\u53d7\u7406\u5e73\u53f0\u5f00\u53d1\uff0c\u56e2\u961f\u6210\u5458\u5305\u62ec\u7ec4\u957f\u8001\u5f20\uff0c\u7ec4\u5458\u5c0f\u674e\u3001\u5c0f\u90ed\u3001\u5c0f\u5f20\u3001\u5c0f\u5b54\u3001\u5c0f\u8bb8\u3001\u5c0f\u6731\u3002 \u5728\u8fc7\u53bb\u4e00\u5468\u7684\u5de5\u4f5c\u4e2d\uff0c\u6211\u5bf9\u9879\u76ee\u8fdb\u884c\u4e86\u8be6\u7ec6\u7684\u5f00\u53d1\u5de5\u4f5c\u3002\u5177\u4f53\u60c5\u51b5\u5982\u4e0b: 1.\u517b\u8001\u7f34\u8d39\u51ed\u8bc1\u6253\u5370\u529f\u80fd:\u5df2\u5b8c\u6210\uff0c\u65e0\u5ef6\u8fdf\u3002 2.\u5931\u4e1a\u91d1\u7ebf\u4e0a\u7533\u9886\u529f\u80fd: \u5df2\u5b8c\u6210\uff0c\u65e0\u5ef6\u8fdf\u3002 3.\u518d\u5c31\u4e1a\u4e0d\u6ee1\u4e00\u5e74\u5f85\u9047\u6838\u5b9a\u529f\u80fd:\u5df2\u5b8c\u6210\uff0c\u65e0\u5ef6\u8fdf\u3002 4.\u6280\u80fd\u63d0\u5347\u8865\u8d34\u7533\u9886\u529f\u80fd:\u5df2\u5b8c\u6210 90%\u7684\u5f00\u53d1\u5de5\u4f5c\uff0c\u5269\u4f59 10%\u9700\u8981\u89e3\u51b3\u8bc1\u4e66\u4fe1\u606f\u83b7\u53d6\u63a5\u53e3\u7684\u95ee\u9898\u3002 \u672c\u5468\u9047\u5230\u7684\u5173\u952e\u95ee\u9898\uff1a\u5931\u4e1a\u4fdd\u9669\u670d\u52a1-\u6280\u80fd\u63d0\u5347\u8865\u8d34\u7533\u9886\u6a21\u5757\u4e2d\u8bc1\u4e66\u4fe1\u606f\u83b7\u53d6\u63a5\u53e3\u672a\u77e5\u3002\u4e3a\u4e86\u89e3\u51b3\u8fd9\u4e2a\u95ee\u9898\uff0c\u6211\u8ba1\u5212\u5728\u4e0b\u5468\u7740\u624b\u7814\u7a76\u5e76\u5bfb\u627e\u89e3\u51b3\u65b9\u6848\u3002 \u6211\u7684\u4e0b\u5468\u8ba1\u5212\uff1a\u5f00\u53d1\u517b\u8001\u4fdd\u9669\u670d\u52a1\u8f6c\u5165\u7533\u8bf7\u529f\u80fd\u5e76\u5bf9\u672c\u5468\u5df2\u5f00\u53d1\u5b8c\u6210\u529f\u80fd\u8fdb\u884c\u5355\u5143\u6d4b\u8bd5\u53ca bug \u4fee\u6539\u3002 \u9664\u4e86\u4ee5\u4e0a\u7684\u5de5\u4f5c\u9879\u76ee,\u6211\u8fd8\u53c2\u4e0e\u4e86\u56e2\u961f\u7684\u8ba8\u8bba\u4f1a\u8bae,\u5e76\u8fdb\u884c\u4e86\u5173\u4e8e\u65b0\u529f\u80fd\u7684\u57f9\u8bad\u3002\u9644\u4ef6\u63d0\u4f9b\u4e86\u672c\u5468\u5de5\u4f5c\u7684\u8be6\u7ec6\u62a5\u544a\uff0c\u8bf7\u60a8\u548c\u56e2\u961f\u6210\u5458\u53c2\u8003\u3002 \u795d\u60a8\u5de5\u4f5c\u6109\u5feb! 2020\u5e747\u670810\u65e5 \u5f00\u53d1\u4eba\u5458\u5c0f\u767d \u5f00\u53d1\u90e8\u533b\u4fdd\u7ec4 \u6cb3\u5317\u4e1c\u8f6f\u8f6f\u4ef6\u6709\u9650\u516c\u53f8 \u5355\u4f4d\u5730\u5740\uff1a\u79e6\u7687\u5c9b\u5e02\u7ecf\u6d4e\u6280\u672f\u5f00\u53d1\u533a\u6cf0\u5c71\u8def143\u53f7\u7efc\u5408\u5b9e\u9a8c\u697c15\u697c \u90ae\u7f16\uff1a066004 \u8054\u7cfb\u7535\u8bdd\uff1a0335-8529605 \u79fb\u52a8\u7535\u8bdd\uff1a15865478965 \u7535\u5b50\u90ae\u4ef6\uff1axiaobai@neuqsoft.com","title":"\u573a\u666f8\uff1a\u5de5\u4f5c\u8fdb\u5ea6\u6c47\u62a5"}]}